		"terminal.ansiBrightRed": "#FF9595",
		"terminal.ansiWhite": "#DFDFDF",
		"terminal.ansiBrightWhite": "#DFDFDF",
		"terminal.ansiYellow": "#FFF680",
		"terminal.ansiBrightYellow": "#FFF680",
		"terminal.selectionBackground": "#555555",
		"terminalCursor.background": "#FFC580",
		"terminalCursor.foreground": "#FFC580"
